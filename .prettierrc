{
  "printWidth": 999,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "overrides": [
    {
      "files": ["*.js"],
      "options": {
        "parser": "babel",
        "htmlWhitespaceSensitivity": "ignore",
        "arrowParens": "avoid"
      }
    }
  ]
}